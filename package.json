{
  "name": "phantom-backend-elites",
  "version": "1.0.0",
  "description": "A backend for Phantom appalication",
  "main": "/home/linuxlite/code/Phantom-Backend-Elites/Phantom-Backend-Elites/src/app.js/",
  "scripts": {
    "build": "babel --copy-files src --out-dir dist",
    "start": "npm run build && node dist/server.js",
    "pre-dev": "babel-node src/server.js",
    "dev": "nodemon --exec npm run pre-dev",
    "test": "mocha --require @babel/register --timout 10000",
    "type": "modele",
    "start:dev": "nodemon --exec babel-node src/app.js",
    "coverage": "nyc mocha --coverage",
    "test:watch": "mocha --watch --reporter min",
    "format": "prettier --write \"src/**/*.{js, mjs}\"",
    "lint": "eslint \"src/**/*.{js, mjs}\" --quiet",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "make-seed": "npx sequelize-cli seed:generate --name demo-user"
  },
  "mocha": {
    "coverageReport": [
      "text-summary",
      "lcov"
    ],
    "collectCoverage": true,
    "testEnvironment": "jsdom"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/Phantom-Backend-Elites.git"
  },
