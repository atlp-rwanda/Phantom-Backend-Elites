    "i18next": "^11.1.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-express-middleware": "^2.0.0",
    "i18next-fs-backend": "^1.1.4",
    "i18next-node-fs-backend": "^2.1.3",
    "jsdom": "^19.0.0",
    "jsdom-global": "^3.0.2",
    "nodemailer": "^6.7.2",
    "npm": "^8.5.1",
