{
  "name": "phantom-backend-elites",
  "version": "1.0.0",
  "description": "A backend for Phantom appalication",
  "main": "server.js",
  "scripts": {
    "build": "babel --copy-files src --out-dir dist",
    "start": "node dist/app.js",
    "start:dev": "nodemon --exec babel-node app.js",
    "test": "mocha --timout 10000",
    "dev": "parcel src/server.js",
    "format": "prettier --write \"src/**/*.{js, mjs}\"",
    "lint": "eslint \"src/**/*.{js, mjs}\" --quiet",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "make-seed": "npx sequelize-cli seed:generate --name demo-user"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/Phantom-Backend-Elites.git"
  },
  "author": "code-elites",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/Phantom-Backend-Elites/issues"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "fs": "^0.0.1-security",
    "i18n": "^0.14.1",
    "i18n-express": "^1.1.3",
    "i18next": "^11.1.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-express-middleware": "^2.0.0",
    "i18next-fs-backend": "^1.1.4",
    "i18next-node-fs-backend": "^2.1.3",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "process": "^0.11.10",
    "sequelize": "^6.16.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.5",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "chai": "^4.3.6",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.1.0",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "parcel": "^1.12.3",
    "prettier": "^2.5.1",
    "sequelize-cli": "^6.4.1",
    "sinon": "^13.0.1"
  }
}
