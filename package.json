{
  "name": "phantom-backend-elites",
  "version": "1.0.0",
  "description": "A backend for Phantom appalication",
  "main": "server.js",
  "scripts": {
    "build": "babel --copy-files src --out-dir dist",
    "start": "npm run build && node dist/server.js",
    "pre-dev": "babel-node src/server.js",
    "dev": "cross-env NODE_ENV=development nodemon --exec npm run pre-dev",
    "format": "prettier --write \"src/**/*.{js, mjs}\"",
    "lint": "eslint \"src/**/*.{js, mjs}\" --quiet",
    "migrate": "npx sequelize-cli db:migrate",
    "down": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seeds": "npx sequelize-cli db:seed:undo",
    "make-seed": "npx sequelize-cli seed:generate --name demo-user",
    "test": "cross-env NODE_ENV=test nyc mocha --timeout 40000 ./test/*.test.js --require @babel/register --exit",
    "pretest": "cross-env NODE_ENV=test npm run down && cross-env NODE_ENV=test npm run migrate && cross-env NODE_ENV=test npm run seed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlp-rwanda/Phantom-Backend-Elites.git"
  },
  "author": "code-elites",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/atlp-rwanda/Phantom-Backend-Elites/issues"
  },
  "homepage": "https://github.com/atlp-rwanda/Phantom-Backend-Elites#readme",
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "debug": "^4.3.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "fs": "^0.0.1-security",
    "i18n": "^0.14.1",
    "i18n-express": "^1.1.3",
    "i18next": "^11.1.11",
    "i18next-browser-languagedetector": "^6.1.3",
    "i18next-express-middleware": "^2.0.0",
    "i18next-fs-backend": "^1.1.4",
    "i18next-node-fs-backend": "^2.1.3",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "node-geocoder": "^4.0.0",
    "nodemailer": "^6.7.2",
    "path": "^0.12.7",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "process": "^0.11.10",
    "request": "^2.88.2",
    "sequelize": "^6.16.1",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.0",
    "@babel/core": "^7.17.2",
    "@babel/node": "^7.16.8",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.16.11",
    "@babel/runtime": "^7.17.2",
    "babel-cli": "^6.26.0",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.1.0",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.5.1",
    "sequelize-cli": "^6.4.1",
    "sinon": "^13.0.1",
    "supertest": "^6.2.2"
  }
}
